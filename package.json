{
  "name": "early-birds-technicaltest",
  "version": "1.0.0",
  "description": "import product from csv - use google cloud vision on them to get dominant color - api that return similar products",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --ignore dist/ --ignore yarn.lock --ignore package.json --exec \"npm start\"",
    "start": "npm run build && npm run serve",
    "build": "babel src --out-dir dist",
    "serve": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "coverage",
        {
          "only": "src/*/"
        }
      ],
      "transform-runtime"
    ]
  },
  "author": "jvolonda",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-register": "^6.24.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-coverage": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validator": "^5.1.0",
    "filter-object": "^3.0.0",
    "morgan": "^1.9.0",
    "winston": "^2.4.1"
  }
}
